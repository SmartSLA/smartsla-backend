  style.
    * {
    box-sizing: border-box;
    }
    body {
    margin: 0;
    padding: 0;
    }
    .column-1[data-isprivate="private"] {
      border-left: 3px solid #f9acb4;
    }
    .column-1[data-isprivate="public"] {
      border-left: 3px solid #2196f3;
    }

    a[x-apple-data-detectors] {
    color: inherit !important;
    text-decoration: inherit !important;
    }
    #MessageViewBody a {
    color: inherit;
    text-decoration: none;
    }
    p {
    line-height: inherit
    }
    @media (max-width:670px) {
    .icons-inner {
    text-align: center;
    }
    .icons_avatar {
      display: none;
    }
    .column-1 {
       padding-left: 6px;
    }
    [data-isprivate="private"] {
      border-left: 3px solid #f9acb4;
    }
    [data-isprivate="public"] {
      border-left: 3px solid #2196f3;
    }
    .icons-inner td {
    margin: 0 auto;
    }
    .row-content {
    width: 100% !important;
    }
    .column .border {
    display: none;
    }
    table {
    table-layout: fixed !important;
    }
    .stack .column {
    width: 100%;
    display: block;
    }
    }
